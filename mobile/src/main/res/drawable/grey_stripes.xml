        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/grey" />
        </shape>
    </item>
    <item>
        <bitmap
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:src="@drawable/bgstripes"
        android:tileMode="repeat"
        android:dither="true" />
    </item>
</layer-list>